<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <label for="Name">Name</label>
  <%= f.text_field :name, autofocus: true, id:"Name", placeholder:"名前" %>
  <label for="Introduction">Introduction</label>
  <%= f.text_field :introduction, placeholder: "introduction" %>
  <%= f.submit "変更を保存" %>
<% end %>
