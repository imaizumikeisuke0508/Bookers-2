<div class="container">
  <div class="row">

  <div class="col-lg-4">
  <%= render 'currentuser' %>
  <%= render 'books/create' %>
  </div>

<div class="col-lg-8">
<table class="table table-striped">
<h2>Users</h2>
<% @users.each do |user| %>
<%= link_to user_path(user.id) do %>
<%= image_tag user.get_profile_image(100,100) %>
<%= user.name %>
<% end %>
<%= user.introduction %>
<% end %>
</table>
</div>

</div>
</div>